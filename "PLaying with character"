#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char ch;
    char s1[100];
    char sen[122];
    
    
     scanf("%c\n%s\n%[^\n]s",&ch,s1,sen);
    printf("%c\n%s\n%s",ch,s1,sen);
    return 0;
    
}
